	<properties>
		<java.version>1.8</java.version>
	</properties>
	<repositories>
		<repository>
			<id>mesir-repo</id>
			<url>http://mesir.googlecode.com/svn/trunk/mavenrepo</url>
		</repository>
		<repository>
			<id>commons-lang3</id>
			<url>http://dist.wso2.org/maven2/</url>
		</repository>
	</repositories>
	<dependencies>
